+class LineChart {
+  constructor(ctx, labels, values) {
+    this.chart = new Chart(ctx, {
+      type: "line",
+      data: {
+        labels: labels,
+        datasets: [
+          {
+            label: "Wally",
+            data: values,
+            borderColor: "#4c51bf",
+          },
+        ],
+      },
+      options: {
+        scales: {
+          xAxes: [
+            {
+              ticks: {
+                fontStyle: "bold",
+                fontSize: 14,
+              },
+            },
+          ],
