+                suggestedMin: 50,
+                suggestedMax: 200,
+                fontStyle: "bold",
+                fontSize: 14,
+              },
+            },
+          ],
+        },
+      },
+    });
+  }
+
+  addPoint(label, value) {
+    const labels = this.chart.data.labels;
+    const data = this.chart.data.datasets[0].data;
+
+    labels.push(label);
+    data.push(value);
+
+    if (data.length > 12) {
+      data.shift();
+      labels.shift();
+    }
+
+    this.chart.update();
