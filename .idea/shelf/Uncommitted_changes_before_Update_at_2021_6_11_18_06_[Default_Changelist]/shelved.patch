+let Hooks = {};
+
+Hooks.IncidentMap = {
+  mounted() {
+    this.map = new IncidentMap(this.el, [39.74, -104.99], event => {
+      const incidentId = event.target.options.incidentId;
+
+      this.pushEvent("marker-clicked", incidentId, (reply, ref) => {
+        this.scrollTo(reply.incident.id)
+      })
